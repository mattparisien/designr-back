{
  "testEnvironment": "node",
  "transform": {},
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ],
  "collectCoverageFrom": [
    "agent/**/*.js",
    "services/projectAgentService.js",
    "!**/node_modules/**",
    "!**/dist/**"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "html",
    "lcov"
  ],
  "testTimeout": 30000,
  "clearMocks": true,
  "resetMocks": true,
  "restoreMocks": true
}
