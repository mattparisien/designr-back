{
  "name": "back",
  "version": "1.0.0",
  "description": "Backend server for Canva clone presentation persistence",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "vector": "node scripts/vector-manager.js",
    "fix-folders": "node scripts/fix-root-folders.js"
  },
  "keywords": [
    "canva",
    "presentations",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/anthropic": "^0.3.23",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.61",
    "@langchain/langgraph": "^0.3.5",
    "@langchain/openai": "^0.5.15",
    "@langchain/tavily": "^0.1.3",
    "@pinecone-database/pinecone": "^6.1.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "form-data": "^4.0.3",
    "fs": "^0.0.1-security",
    "hnswlib-node": "^3.0.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "langsmith": "^0.3.33",
    "mongoose": "^6.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-gridfs-storage": "^5.0.2",
    "node-tesseract-ocr": "^2.2.1",
    "openai": "^4.104.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.34.2",
    "tesseract.js": "^6.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.3",
    "jest": "^30.0.2",
    "nodemon": "^2.0.22",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "collectCoverageFrom": [
      "services/**/*.js",
      "controllers/**/*.js",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/*.test.js",
      "!**/dist/**",
      "!**/node_modules/**"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "resolutions": {
    "bson": "5.3.0"
  }
}
