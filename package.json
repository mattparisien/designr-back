{
  "name": "back",
  "version": "1.0.0",
  "description": "Backend server for Canva clone presentation persistence",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "tsx server.js",
    "start:ts": "ts-node server.ts",
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r models dist/ && cp -r config dist/ && cp -r utils dist/ && cp -r middleware dist/ && cp -r services dist/ && cp -r routes dist/ && cp server.js dist/",
    "dev": "nodemon --exec tsx server.js",
    "dev:ts": "nodemon --exec ts-node server.ts",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:agent": "node --experimental-vm-modules ./node_modules/.bin/jest --testPathPattern=tests/agent --verbose",
    "test:agent:watch": "node --experimental-vm-modules ./node_modules/.bin/jest --testPathPattern=tests/agent --watch",
    "test:agent:coverage": "node --experimental-vm-modules ./node_modules/.bin/jest --testPathPattern=tests/agent --coverage --collectCoverageFrom=agent/**/*.js --collectCoverageFrom=services/projectAgentService.js",
    "test:agent:run": "node tests/agent/demo-workflow.js",
    "test:agent:simple": "node tests/agent/simple-integration-test.js",
    "test:agent:ts": "tsx tests/agent/index.ts",
    "vector": "node scripts/vector-manager.js",
    "fix-folders": "node scripts/fix-root-folders.js"
  },
  "keywords": [
    "canva",
    "presentations",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@canva-clone/shared-types": "file:../../packages/shared",
    "@openai/agents": "^0.0.7",
    "@pinecone-database/pinecone": "^6.1.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "fs": "^0.0.1-security",
    "hnswlib-node": "^3.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "mongoose": "^6.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-gridfs-storage": "^5.0.2",
    "openai": "^4.98.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "sharp": "^0.34.1",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  },
  "resolutions": {
    "bson": "5.3.0"
  }
}
